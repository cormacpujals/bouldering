<%- include('../partials/header') %>

<h2>Look at all of these cool posts.</h2>


<table>
    <thead>
    <tr>
        <th>Photo</th>
        <th>Name</th>
        <th>Grade</th>
        <th>Location</th>
    </tr>
    </thead>
    <tbody>
    <% posts.forEach(function(p) { %>
        <tr>
            <td><img src="<%= p.img %>"/></td>
            <td><%= p.name %></td>
            <td><%= p.grade %></td>
            <td><%= p.location %></td>
            <td><a href="/posts/<%= p._id %>">Read more</a></td>
        </tr>
    <% }); %>
    </tbody>
</table>



<%- include('../partials/footer') %>
